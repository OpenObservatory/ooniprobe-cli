    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "camelcase": "^4.1.0",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.1.0",
    "flow-babel-webpack-plugin": "^1.1.0",
    "fs-extra": "^4.0.2",
    "inquirer": "^3.3.0",
    "lodash.range": "^3.2.0",
    "mri": "^1.1.0",
    "ora": "^1.3.0",
    "pkg": "^4.2.4",
    "shebang-loader": "^0.0.1",
    "string-length": "^2.0.0",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0",
    "wrap-ansi": "^3.0.1"
  },
  "dependencies": {
  }
