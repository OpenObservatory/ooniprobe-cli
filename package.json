{
  "name": "ooniprobe",
  "version": "3.0.0-alpha.0",
  "description": "The command line interface for OONI Probe",
  "main": "src/ooni.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack -w",
    "pack": "webpack && pkg dist/ooni.js -c package.json -o packed/ooni"
